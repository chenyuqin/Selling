<div id="donor_info" class="donor_info">
    <div class="weui_cell">
        <div class="weui_cell_hd">
            <label class="weui_label">姓名</label>
        </div>
        <div class="weui_cell_bd weui_cell_primary">
            <input id="donor_name" name="donor_name" class="weui_input" type="text" placeholder="请填写本人姓名" autocomplete="off"/>
        </div>
    </div>
    <div class="weui_cell">
        <div class="weui_cell_hd">
            <label class="weui_label">手机</label>
        </div>
        <div class="weui_cell_bd weui_cell_primary">
            <input id="donor_phone" name="donor_phone" class="weui_input" type="tel" placeholder="请填写本人手机号" autocomplete="off"/>
        </div>
    </div>
</div>