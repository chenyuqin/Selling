<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="commodityDao" class="selling.sunshine.dao.impl.CommodityDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="agentDao" class="selling.sunshine.dao.impl.AgentDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="adminDao" class="selling.sunshine.dao.impl.AdminDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="userDao" class="selling.sunshine.dao.impl.UserDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="customerDao" class="selling.sunshine.dao.impl.CustomerDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="orderDao" class="selling.sunshine.dao.impl.OrderDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="orderItemDao" class="selling.sunshine.dao.impl.OrderItemDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>

    <bean id="shipmentDao" class="selling.sunshine.dao.impl.ShipmentDaoImpl">
        <property name="sqlSession" ref="sqlSession"></property>
    </bean>
</beans>