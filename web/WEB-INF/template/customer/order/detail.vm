#parse("/common/util.vm")
<html lang="zh_CN">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0, user-scalable=no">
    <link rel="stylesheet" href="${path.concat('/plugins/weui/weui.css')}"/>
    <link rel="stylesheet" href="${path.concat('/material/css/customer/order_detail.css')}"/>
    <script type="text/javascript" src="${path.concat('/plugins/jquery/jquery-1.12.3.min.js')}"></script>
    <title>我的订单</title>
</head>
<body>
    <div class="my_order">
        <div class="hd">
            <h1 class="page_title">我的订单${customerOrder.orderId}</h1>
        </div>
        <div class="bd">
            <div class="weui_cells">
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>订单号</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>商品名${customerOrder.goods.name}</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>代理商</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>数量</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>总金额</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>收件人</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>手机</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <p>地址</p>
                    </div>
                    <div class="weui_cell_ft">
                        <p></p>
                    </div>
                </div>
                <div class="weui_cell">
                    <div class="weui_cell_bd weui_cell_primary">
                        <a href="#">查看当前订单状态</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
<script>
    
</script>
</html>
