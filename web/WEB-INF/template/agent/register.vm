#parse("/agent/util.vm")
<html>
<head>
    #parse("/agent/component/init.vm")
    <title>代理注册</title>
</head>
<body ontouchstart>
<div class="container">
    <div class="hd">
        <h1 class="page_title">注册</h1>

        <p class="page_desc">注册申请成为销售代理商</p>
    </div>
    <div class="bd">
        <div class="weui_cells weui_cells_form">
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">姓名</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input class="weui_input" type="text" placeholder="请填写姓名"/>
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">手机</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input class="weui_input" type="number" pattern="1[0-9]{10}" placeholder="请填写手机号"/>
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">地址</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    <input class="weui_input" type="text" placeholder="请填写联系地址"/>
                </div>
            </div>
            <div class="weui_cell">
                <div class="weui_cell_hd">
                    <label class="weui_label">称谓</label>
                </div>
                <div class="weui_cell_bd weui_cell_primary">
                    
                </div>
            </div>
        </div>
    </div>
</div>
</div>
</body>
</html>